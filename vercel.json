{
    "version": 2,
    "builds": [
      { "src": "/backend/api/index.js", "use": "@vercel/node" },
      { "src": "/my-proj-database", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/backend/api/(.*)", "dest": "/backend/api/index.js" },
      { "src": "/backend/(.*)", "dest": "/backend/api/index.js" },
      { "src": "/(.*)", "dest": "/my-proj-database/$1" }
    ]
  }
  